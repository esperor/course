{
  "name": "course.client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm:tailwind:watch\"",
    "build": "npm run tailwind && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tailwind:watch": "npm run tailwind -- --watch",
    "tailwind": "npx tailwindcss -i ./src/index.css -o ./src/tailwind.css"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.40.1",
    "@tanstack/react-router": "^1.35.1",
    "axios": "^1.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.35.6",
    "@tanstack/react-router-devtools": "^1.158.4",
    "@tanstack/router-vite-plugin": "^1.34.8",
    "@types/node": "^20.12.13",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "5.9",
    "vite": "^7.2.6"
  }
}
